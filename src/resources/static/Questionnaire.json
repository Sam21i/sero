      "item": [
        {
          "linkId": "4.1",
          "type": "text",
          "repeats": false,
          "required": false,
          "text": "Welche Gefühle spürst du, wenn du den Drang, dir das Leben zu nehmen, dort siehst?",
          "_text": {
            "extension": [
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "de"
                  },
                  {
                    "url": "content",
                    "valueString": "Welche Gefühle spürst du, wenn du den Drang, dir das Leben zu nehmen, dort siehst?"
                  }
                ]
              },
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "fr"
                  },
                  {
                    "url": "content",
                    "valueString": "Quels sentiments ressens-tu lorsque tu vois l'envie de mettre fin à tes jours à la position choisie?"
                  }
                ]
              },
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "it"
                  },
                  {
                    "url": "content",
                    "valueString": "Quali sentimenti prova quando vede l'impulso a togliersi la vita?"
                  }
                ]
              }
            ]
          }
        }
      ]
    },

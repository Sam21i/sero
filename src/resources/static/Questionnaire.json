      "item": [
        {
          "linkId": "2.1",
          "type": "text",
          "repeats": false,
          "required": false,
          "text": "Was geht dir durch den Kopf, wenn du die Scheibe dort platzierst?",
          "_text": {
            "extension": [
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "de"
                  },
                  {
                    "url": "content",
                    "valueString": "Was ist mir gerade durch den Kopf gegangen, bevor ich die Scheibe dort platziert habe?"
                  }
                ]
              },
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "fr"
                  },
                  {
                    "url": "content",
                    "valueString": "Qu'est-ce qui m'a traversé l'esprit avant que je place le disque à cet endroit?"
                  }
                ]
              },
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "it"
                  },
                  {
                    "url": "content",
                    "valueString": "Cosa mi è passato per la testa prima di mettere il disco lì?"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    {

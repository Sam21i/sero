                    "linkId": "4.1",
                    "type": "text",
                    "repeats": false,
                    "required": false, 
                    "text": "Welche Gefühle tauchen in mir auf, wenn ich den Drang, mir das Leben zu nehmen, an diesem Platz sehe?",
                    "_text": {
                        "extension": [
                            {
                                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                                "extension": [
                                    {
                                        "url": "lang",
                                        "valueCode": "de"
                                    },
                                    {
                                        "url": "content",
                                        "valueString": "Welche Gefühle tauchen in mir auf, wenn ich den Drang, mir das Leben zu nehmen, an diesem Platz sehe?"
                                    }
                                ]
                            }
                        ]
                    }
                }
            ]
        },
        {
            "linkId": "5",
            "type": "group",
            "repeats": false,
            "required": false, 
            "text": "Fragen zur Selbstreflexion",
            "_text": {
                "extension": [
                    {
                        "url": "http://hl7.org/fhir/StructureDefinition/translation",
                        "extension": [
                            {
                                "url": "lang",
                                "valueCode": "de"
                            },

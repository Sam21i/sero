          "linkId": "5.1",
          "type": "text",
          "repeats": false,
          "required": false,
          "text": "Was hält den Suizid-Drang (schwarze Scheibe) gerade von dir weg?",
          "_text": {
            "extension": [
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "de"
                  },
                  {
                    "url": "content",
                    "valueString": "Was hält den Suizid-Drang (schwarze Scheibe) gerade von dir weg?"
                  }
                ]
              },
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "fr"
                  },
                  {
                    "url": "content",
                    "valueString": "Qu'est-ce qui éloigne ta pulsion suicidaire (disque noir) en ce moment?"
                  }
                ]
              },
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "it"
                  },
                  {
                    "url": "content",
                    "valueString": "Che cosa tiene lontano il tuo impulso al suicidio (disco nero) in questo momento?"
                  }
                ]
              }
            ]
          }
        }

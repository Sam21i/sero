    "item": [
        {
            "linkId": "1",
            "type": "text",
            "repeats": false,
            "required": false, 
            "text": "Sehen Sie sich nun die Distanz zwischen dem gelben Punkt (Ihnen selbst) und der schwarzen Scheibe (dem Drang sich das Leben zu nehmen) an. Was bedeutet es für Sie, dass Sie die schwarze Scheibe dort platziert haben?",
            "_text": {
                "extension": [
                    {
                        "url": "http://hl7.org/fhir/StructureDefinition/translation",
                        "extension": [
                            {
                                "url": "lang",
                                "valueCode": "de"
                            },
                            {
                                "url": "content",
                                "valueString": "Sehen Sie sich nun die Distanz zwischen dem gelben Punkt (Ihnen selbst) und der schwarzen Scheibe (dem Drang sich das Leben zu nehmen) an. Was bedeutet es für Sie, dass Sie die schwarze Scheibe dort platziert haben?"
                            }
                        ]
                    }
                ]
            }
        },
        {
            "linkId": "2",
            "type": "group",
            "repeats": false,
            "required": false, 
            "text": "Fragen zu Gedanken",
            "_text": {
                "extension": [
                    {
                        "url": "http://hl7.org/fhir/StructureDefinition/translation",
                        "extension": [
                            {
                                "url": "lang",
                                "valueCode": "de"

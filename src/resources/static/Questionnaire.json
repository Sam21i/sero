  "item": [
    {
      "linkId": "1",
      "type": "text",
      "repeats": false,
      "required": false,
      "text": "Was bedeutet es für dich, dass du die schwarze Scheibe gerade dort platziert hast? Was hat damit zu tun?",
      "_text": {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/translation",
            "extension": [
              {
                "url": "lang",
                "valueCode": "de"
              },
              {
                "url": "content",
                "valueString": "Was bedeutet es für dich, dass du die schwarze Scheibe gerade dort platziert hast? Was hat damit zu tun?"
              }
            ]
          },
          {
            "url": "http://hl7.org/fhir/StructureDefinition/translation",
            "extension": [
              {
                "url": "lang",
                "valueCode": "fr"
              },
              {
                "url": "content",
                "valueString": "Que signifie pour toi le fait d'avoir placé le disque noir à cet endroit précis ? Quel est le rapport?"
              }
            ]
          },
          {
            "url": "http://hl7.org/fhir/StructureDefinition/translation",
            "extension": [
              {
                "url": "lang",
                "valueCode": "it"
              },
              {
                "url": "content",
                "valueString": "Che cosa significa per voi il fatto che abbiate appena messo lì il disco nero? Cosa c'entra?"
              }
            ]
          }
        ]
      }
    },
    {

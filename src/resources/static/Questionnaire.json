      "item": [
        {
          "linkId": "3.1",
          "type": "text",
          "repeats": false,
          "required": false,
          "text": "Was nimmst du gerade im Körper wahr?",
          "_text": {
            "extension": [
              {
                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                "extension": [
                  {
                    "url": "lang",
                    "valueCode": "de"
                  },
                  {
                    "url": "content",
                    "valueString": "Was nimmst du gerade im Körper wahr?"

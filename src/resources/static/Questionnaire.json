                    }
                ]
            },
            "item": [
                {
                    "linkId": "2.1",
                    "type": "text",
                    "repeats": false,
                    "required": false, 
                    "text": "Was ist mir gerade durch den Kopf gegangen, bevor ich die Scheibe dort platziert haben?",
                    "_text": {
                        "extension": [
                            {
                                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                                "extension": [
                                    {
                                        "url": "lang",
                                        "valueCode": "de"
                                    },
                                    {
                                        "url": "content",
                                        "valueString": "Was ist mir gerade durch den Kopf gegangen, bevor ich die Scheibe dort platziert haben?"
                                    }
                                ]
                            }
                        ]
                    }
                },
                {
                    "linkId": "2.2",
                    "type": "text",
                    "repeats": false,
                    "required": false, 
                    "text": "Welche Gedanken habe ich jetzt, wenn ich die Scheibe dort liegen sehe?",
                    "_text": {
                        "extension": [
                            {
                                "url": "http://hl7.org/fhir/StructureDefinition/translation",
                                "extension": [
                                    {
                                        "url": "lang",
                                        "valueCode": "de"
                                    },
                                    {
                                        "url": "content",
                                        "valueString": "Welche Gedanken habe ich jetzt, wenn ich die Scheibe dort liegen sehe?"
                                    }
                                ]
                            }
                        ]
                    }
                }
            ]
        },
        {
            "linkId": "3",
            "type": "group",
            "repeats": false,
            "required": false, 
            "text": "Fragen zur Körperwahrnehmung",
            "_text": {
